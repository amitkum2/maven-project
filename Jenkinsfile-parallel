pipeline{
    agent any
    stages
    {
        stage('Parallel Deployment')
        {
            parallel
            {
                 stage('Deploy to QA')
                 {

                    steps
                    {
                    echo 'Deploying to QA'
                    }
                }
            }
    {
        stage('Deploy to Dev')
        {
             steps
            {
             echo 'Deploying to Dev'
            }
        }
    }
        }
        }
    }
